<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_mrs_lab_test_type_form" model="ir.ui.view">
        <field name="name">Lab Test Type</field>
        <field name="model">mrs.lab.test.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name" />
                        </group>
                        <group>
                            <field name="code" />
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_mrs_lab_test_type_list" model="ir.ui.view">
        <field name="name">Lab Test Type</field>
        <field name="model">mrs.lab.test.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>
    <record id="mrs_lab_test_type_action" model="ir.actions.act_window">
        <field name="name">Lab Test Type</field>
        <field name="res_model">mrs.lab.test.type</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Add a new Lab Test Type
            </p><p>
            Create Lab Test Type
            </p>
        </field>
    </record>
</odoo>
