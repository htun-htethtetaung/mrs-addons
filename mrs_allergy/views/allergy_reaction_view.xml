<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_mrs_allergy_reaction_form" model="ir.ui.view">
        <field name="name">Allergy Reaction</field>
        <field name="model">mrs.allergy.reaction</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_mrs_allergy_reaction_list" model="ir.ui.view">
        <field name="name">Allergy Reaction</field>
        <field name="model">mrs.allergy.reaction</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="mrs_allergy_reaction_action" model="ir.actions.act_window">
        <field name="name">Allergy Reaction</field>
        <field name="res_model">mrs.allergy.reaction</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Add a new Allergy Reaction
            </p><p>
            Create Allergy Reaction
            </p>
        </field>
    </record>
</odoo>